# EC2 instance type
instance_type = "t2.micro"
# Amount of nodes in cluster
instance_count = "3"
# DNS Name
dns_domain = "flugel-it.com"
# Hard Disk size
vol_size = "10"
# Linux Distro Flavor (centos or ubuntu)
os_env = "centos"
# Name of the mongodb cluster
cluster_name = "mongocluster"
# SSH Key name created previously
key_name = "Cluster_Automation"
